<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="448.0" prefWidth="611.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.library.controllers.NewAccountController">
   <children>
      <ImageView fitHeight="640.0" fitWidth="727.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../blblioteca.jpg" />
         </image>
      </ImageView>
      <TextField fx:id="name" layoutX="74.0" layoutY="43.0" prefHeight="31.0" prefWidth="263.0" promptText="Your name" />
      <TextField fx:id="username" layoutX="74.0" layoutY="96.0" prefHeight="31.0" prefWidth="263.0" promptText="Yout username" />
      <PasswordField fx:id="password" layoutX="74.0" layoutY="176.0" prefHeight="31.0" prefWidth="263.0" promptText="Password" />
      <PasswordField fx:id="confirmPassword" layoutX="74.0" layoutY="245.0" prefHeight="31.0" prefWidth="263.0" promptText="Confirm your password" />
      <Label fx:id="message" layoutX="454.0" layoutY="335.0" prefHeight="21.0" prefWidth="232.0" text="Click here to create account" textFill="#022813ba" textOverrun="WORD_ELLIPSIS">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="513.0" layoutY="376.0" mnemonicParsing="false" onAction="#loginAction" prefHeight="31.0" prefWidth="159.0" text="SignUp" />
   </children>
</AnchorPane>
